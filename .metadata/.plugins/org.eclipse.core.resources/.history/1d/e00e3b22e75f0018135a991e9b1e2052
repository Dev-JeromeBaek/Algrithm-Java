package Q11;

import java.util.Scanner;

public class Main {

	public static void main(String[] args) {
		Scanner scan = new Scanner(System.in);
		int[] intArr = new int[2];
		for (int i=0; i<intArr.length; i++) {
			intArr[i] = scan.nextInt();
		}
		int coupon = intArr[0];
		int pizza = intArr[1];
		System.out.println(coupon/pizza);
		
		
	}
	
}
