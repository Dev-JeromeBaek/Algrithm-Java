import java.util.Arrays;

public class Q1 {
	
	public int solution(int[] A) {
		
		int result = 0;
		Arrays.sort(A);
		for (int i=0; i<A.length; i++) {
			if (A[i] > 0) {
				for (int j=1; j<1000000; j++) {
					if (A[i] != j) {
						result = j;
						break;
					}
				}
				break;
			}
		}
		return result;
	}
	
	public static void main(String[] args) {
		Q1 q = new Q1();
		int[] array = {1, 3, 6, 4, 1, 2};
		System.out.println(q.solution(array));
	}// main END
}// class END
