		int coupon = intArr[1];
		System.out.println(pizza/coupon);
		int additionalPizza = pizza/coupon;
		int remainderCoupon = pizza%coupon;
		
		if(additionalPizza != 0) {
			
		} else {
			System.out.print(pizza + additionalPizza);
		}
		
	}
	
